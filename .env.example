# .env.example - Copy to .env and fill in your values

# Binance Perpetual API
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_API_SECRET=your_binance_api_secret_here

# Solana
SOLANA_PRIVATE_KEY=your_solana_private_key_base58_or_json_array

# Jupiter Ultra API (optional, defaults to mainnet)
JUPITER_API_URL=https://api.jup.ag/ultra/v1
JUPITER_API_KEY=your_jupiter_api_key_here

# Note: Token mint addresses are now configured in markets.json
# This allows easier management of multiple markets
